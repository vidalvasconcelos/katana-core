{
  "name": "themsaid/katana-core",
  "description": "Static site & blog generator with markdown support.",
  "keywords": [
    "flat-file",
    "cms",
    "static",
    "site",
    "generator",
    "markdown",
    "blade"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mohamed Said",
      "email": "themohamedsaid@gmail.com"
    }
  ],
  "require": {
    "php": "7.3.*",
    "erusev/parsedown": "^1.6",
    "illuminate/view": "5.2.*",
    "mnapoli/front-yaml": "^1.5",
    "symfony/console": "2.6.7",
    "symfony/debug": "2.8.* || 3.0.*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.13"
  },
  "autoload": {
    "psr-4": {
      "Katana\\": "src"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ]
  }
}
