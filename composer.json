{
  "name": "themsaid/katana-core",
  "description": "Static site & blog generator with markdown support.",
  "keywords": [
    "flat-file",
    "cms",
    "static",
    "site",
    "generator",
    "markdown",
    "blade"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mohamed Said",
      "email": "themohamedsaid@gmail.com"
    }
  ],
  "require": {
    "php": "7.3.*",
    "erusev/parsedown": "1.7.*",
    "illuminate/view": "8.37.*",
    "mnapoli/front-yaml": "1.8.*",
    "symfony/console": "5.2.*",
    "symfony/debug": "4.4.*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.13"
  },
  "autoload": {
    "psr-4": {
      "Katana\\": "src"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ]
  }
}
